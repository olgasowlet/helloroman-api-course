lNews';\n\nconst Wrapper = styled.div`\n  padding: 40px;\n  display: grid;\n  grid-template-columns: 0.8fr 1fr;\n  gap: 50px;\n  height: 90vh;\n`\n\nconst Root = () => {\n  return (\n    <Wrapper>\n      <StudentsList />\n      <SchoolNews />\n    </Wrapper>\n  );\n};\n\nexport default Root;\n","/home/olga/Desktop/dev/HelloRoman/API/03/studdybuddy-front/src/views/AddStudent.js",["82","83"],"import React, { useState, useEffect } from 'react';\nimport styled from 'styled-components';\nimport StudentForm from 'components/StudentForm/StudentForm';\n\nconst Wrapper = styled.div`\n  padding: 40px;\n  max-width: 800px;\n`\n\nconst AddStudent = () => {\n  return (\n    <Wrapper>\n      <StudentForm />\n    </Wrapper>\n  );\n};\n\nexport default AddStudent;\n","/home/olga/Desktop/dev/HelloRoman/API/03/studdybuddy-front/src/components/Navigation/Navigation.js",[],"/home/olga/Desktop/dev/HelloRoman/API/03/studdybuddy-front/src/components/Navigation/Navigation.styles.js",[],"/home/olga/Desktop/dev/HelloRoman/API/03/studdybuddy-front/src/components/StudentsList/StudentsList.js",[],"/home/olga/Desktop/dev/HelloRoman/API/03/studdybuddy-front/src/components/StudentForm/StudentForm.js",["84","85","86","87"],"import React, { useState, useEffect, useRef } from 'react';\nimport PropTypes from 'prop-types';\nimport { useForm } from 'react-hook-form';\nimport { EditButton } from 'components/EditButton';\nimport { Average } from 'components/Average';\nimport { getGradesAvg } from 'helpers/gradesAvg';\nimport * as yup from 'yup';\nimport { Title, Wrapper } from 'components/SchoolNews/SchoolNews.styles';\nimport {\n  AddScoreWrapper,\n  Field,\n  Form,\n  Input,\n  Label, SaveButton,\n  Scores,\n  ScoresWrapper\n} from 'components/StudentForm/StudentForm.styles';\nimport { api, endpoints } from 'api';\n\nlet grade = yup.number();\ngrade\n  .integer('Ocena musi być liczbowa')\n  .positive('Ocena musi być liczbą dodatnią')\n  .min(1, 'Ocena musi być wyższa od 0')\n  .max(6, 'Ocena musi być niższa od 7');\n\nconst StudentForm = () => {\n  const [scores, setScores] = useState([]);\n  const [scoreInputValue, setScoreInputValue] = useState('');\n  const { register, handleSubmit, watch, errors } = useForm();\n  const [average, setAverage] = useState();\n\n  const onSubmit = ({ name, age, group }) => {\n    api.post(endpoints.users, {\n      name,\n      group,\n      age: parseInt(age, 10),\n      grades: scores,\n    })\n      .then(response => {\n        console.log(response);\n      })\n      .catch( error => {\n        console.log(error);\n      })\n  };\n\n  useEffect(() => {\n    setAverage(getGradesAvg(scores));\n  }, [scores]);\n\n  const handleScoreInputChange = (e) => {\n    setScoreInputValue(e.target.value);\n  };\n\n  const handleAddGrade = () => {\n    setScores([...scores, parseInt(scoreInputValue)]);\n    setScoreInputValue('');\n  };\n\n  return (\n    <Wrapper>\n      <Title>Dodaj ucznia</Title>\n      <Form onSubmit={handleSubmit(onSubmit)}>\n        <Field><Label>Imię</Label>\n          <Input id=\"name\" name=\"name\" ref={register}/></Field>\n        <Field><Label>Grupa</Label>\n          <Input id=\"group\" name=\"group\" ref={register}/></Field>\n\n        <Field><Label>Wiek</Label>\n          <Input id=\"age\" name=\"age\" ref={register}/></Field>\n        <Field><Label>Dodaj ocenę</Label>\n          <AddScoreWrapper>\n            <Input id=\"score\" name=\"score\" value={scoreInputValue} onChange={handleScoreInputChange}/>\n            <EditButton type=\"button\" onClick={handleAddGrade}>+</EditButton>\n          </AddScoreWrapper></Field>\n        <ScoresWrapper>\n          <Scores>\n            <p>Oceny</p>\n            <p>{scores.join(', ')}</p>\n          </Scores>\n          <Average value={average}>{average}</Average>\n        </ScoresWrapper>\n        <SaveButton type=\"submit\">zapisz</SaveButton>\n      </Form>\n    </Wrapper>\n  );\n};\n\nStudentForm.propTypes = {};\n\nexport default StudentForm;\n","/home/olga/Desktop/dev